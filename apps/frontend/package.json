{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"pnpm codegen:dev\"",
    "start:prod": "vite preview --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "codegen": "dotenv -c -- graphql-codegen --config codegen.config.ts",
    "codegen:dev": "dotenv -c -- graphql-codegen -w --config codegen.config.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@mantine/core": "^7.10.0",
    "@mantine/form": "^7.10.1",
    "@mantine/hooks": "^7.10.0",
    "@repo/types": "workspace:*",
    "apollo-upload-client": "^18.0.1",
    "clsx": "^2.1.1",
    "dotenv-cli": "^7.4.2",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lazily": "^0.9.2",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-operations": "^4.2.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@mantine/notifications": "^7.10.0",
    "@parcel/watcher": "^2.4.1",
    "@tabler/icons-react": "^3.5.0",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.1.0",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.4"
  }
}
